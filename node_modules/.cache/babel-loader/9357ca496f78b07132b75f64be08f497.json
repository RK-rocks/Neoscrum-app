IAAA,QAAA,EAAA;AACA,MAAA,QAAA,EAAA,QADA;AAEA,MAAA,SAAA,EAAA,SAAA,CAAA,CAAA;AAFA;AALA;AArCA,CAAA","sourcesContent":["<template>\n  <!-- eslint-disable -->\n  <div class=\"login-page pa50\">\n    <h1 class=\"heading \"><label class=\"neo\">Neo</label><label class=\"scrum\">SCRUM</label></h1>\n    <div class=\"loginForm\">\n      <form @submit.prevent=\"loginUserForm\" class=\"shadow-lg p-3 mb-5 bg-white rounded loginForm\">\n        <fieldset>\n          <legend>User-login</legend>\n          <div class=\"form-group\">\n              <label for=\"uname\">Email:</label>\n              <input :class=\"{ error: submitted && $v.email.$invalid }\" type=\"email\" v-model=\"email\"\n                class=\"form-control\" id=\"uname\" placeholder=\"Enter username\" name=\"uname\">\n              <div v-if=\"submitted\">\n                <div v-if=\"!$v.email.required\">\n                  <p class=\"text-red text-left\">Email is required</p>\n                </div>\n              </div>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"pwd\">Password:</label>\n            <input :class=\"{ error: submitted && $v.password.$invalid }\" v-model=\"password\" type=\"password\" class=\"form-control\" id=\"pwd\" placeholder=\"Enter password\" name=\"pswd\">\n            <div v-if=\"submitted\">\n                <div v-if=\"!$v.password.required\">\n                  <p class=\"text-red text-left\">Password is required</p>\n                </div>\n              </div>\n          </div>\n          <button type=\"submit\" class=\"btn btn-primary mr10\">Login</button>\n          <a href=\"admin.html\">Registration</a>\n        </fieldset>\n      </form>\n    </div>\n  </div>\n  <!-- eslint-disable -->\n</template>\n<script>\n\nimport { mapActions, mapGetters } from 'vuex';\nimport { required, minLength } from 'vuelidate/lib/validators';\nimport { postRequest } from '../../services';\n\nexport default {\n  data() {\n    return {\n      submitted: false,\n      email: '',\n      password: '',\n    };\n  },\n  computed: {\n    ...mapGetters({\n      isLogin: 'auth/isLogin',\n    }),\n  },\n  methods: {\n    ...mapActions('auth', [\n      'loginSuccess',\n      'logout',\n    ]),\n    loginUserForm() {\n      this.loginSuccess();\n      this.submitted = true;\n      if (this.$v.$invalid) {\n        return;\n      }\n      eqObj = {\n        email : this.email,\n        password : this.password\n      }\n      postRequest()\n      this.$router.push({ name: '/' });\n      this.$store.dispatch('auth/loginSuccess');\n    },\n    onLogout() {\n      this.logout();\n      // this.$store.dispatch('auth/logout');\n    },\n  },\n  validations: {\n    email: {\n      required,\n      minLength: minLength(4),\n    },\n    password: {\n      required,\n      minLength: minLength(4),\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views/auth"}]}