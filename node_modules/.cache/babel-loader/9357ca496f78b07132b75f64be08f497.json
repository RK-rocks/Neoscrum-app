n=\"dashboard.html\" class=\"shadow-lg p-3 mb-5 bg-white rounded loginForm\">\n        <fieldset>\n          <legend>User-login</legend>\n          <div class=\"form-group\">\n              <label for=\"uname\">Email:</label>\n              <input type=\"email\" v-model=\"email\"\n                v-validate=\"'required|email'\"\n                :class=\"{'has-error': errors.has('email')}\" class=\"form-control\" id=\"uname\" placeholder=\"Enter username\" name=\"uname\">\n              <p v-if=\"errors.has('email')\" class=\"alert-danger\">\n                {{ errors.first('email') }}\n              </p>\n          </div>\n          <div class=\"form-group\">\n              <label for=\"pwd\">Password:</label>\n              <input type=\"password\" class=\"form-control\" id=\"pwd\" placeholder=\"Enter password\" name=\"pswd\">\n          </div>\n          <button type=\"submit\" @click=\"onLogin\" class=\"btn btn-primary mr10\">Login</button>\n          <a href=\"admin.html\">Registration</a>\n        </fieldset>\n      </form>\n    </div>\n  </div>\n  <!-- eslint-disable -->\n</template>\n<script>\n\nimport { mapActions, mapGetters } from 'vuex';\n\nexport default {\n  data() {\n    return {\n\n    };\n  },\n  computed: {\n    ...mapGetters({\n      isLogin: 'auth/isLogin',\n    }),\n  },\n  methods: {\n    ...mapActions('auth', [\n      'loginSuccess',\n      'logout',\n    ]),\n    onLogin() {\n      this.loginSuccess();\n      this.$router.push({ name: '/' });\n      this.$validator.validateAll().then(() => {\n        alert(this.email);\n      }).catch(() => {\n        alert(this.errors.all());\n      });\n      this.$store.dispatch('auth/loginSuccess');\n    },\n    onLogout() {\n      this.logout();\n      // this.$store.dispatch('auth/logout');\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views/auth"}]}