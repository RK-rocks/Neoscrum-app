AAA,MAAA,CAAA,WAAA,EAAA,KAAA,CAAA,IAAA;AACA,gBAAA,QAAA,CAAA,MAAA,CAAA,YAAA,EAAA,KAAA,CAAA,KAAA;AACA,gBAAA,QAAA,CAAA,MAAA,CAAA,eAAA,EAAA,KAAA,CAAA,OAAA;AACA,gBAAA,GAZA,GAYA,0CAZA;AAAA;AAAA,uBAaA,KAAA,CAAA,WAAA,CACA,WADA,CACA,GADA,EACA,QADA,EAEA,KAFA,CAEA,UAAA,KAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,kBAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,QAAA;;AACA,sBAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA,KAAA,OAAA,EAAA;AACA,oBAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA;AACA,iBARA,CAbA;;AAAA;AAaA,gBAAA,MAbA;;AAsBA,oBAAA,MAAA,CAAA,IAAA,CAAA,OAAA,KAAA,MAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,OAAA,CAAA;;AACA,kBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,MAAA;AA1BA;AAAA;;AAAA;AAAA;AAAA;AA4BA,gBAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AA5BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA8BA,KA/BA;AAgCA,IAAA,YAhCA,wBAgCA,KAhCA,EAgCA;AAAA;;AACA,UAAA,KAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;AACA,MAAA,MAAA,CAAA,aAAA,CAAA,KAAA;;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,CAAA,CAAA,MAAA,CAAA,MAAA;AACA,OAFA;AAGA;AAvCA;AAxBA,CAAA","sourcesContent":["<template>\n  <div>\n    <h1 class=\"heading\">\n      <label class=\"neo\">Neo</label>\n      <label class=\"scrum\">SCRUM</label>\n    </h1>\n    <div class=\"loginForm\">\n      <form @submit.prevent=\"register\" class=\"shadow-lg p-3 mb-5 bg-white rounded loginForm\">\n        <fieldset>\n          <legend>Admin</legend>\n          <div class=\"form-group\">\n            <label for=\"name\" class=\"float-left\">Name:</label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              id=\"name\"\n              placeholder=\"Enter Name\"\n              v-model.trim=\"name\"\n              :class=\"{ error: submitted && $v.name.$invalid }\"\n            />\n          </div>\n          <div v-if=\"submitted\">\n            <div v-if=\"!$v.name.required\">\n              <p class=\"text-red text-left\">Name is required</p>\n            </div>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"uname\" class=\"float-left\">Email:</label>\n            <input\n              type=\"email\"\n              class=\"form-control\"\n              id=\"uname\"\n              placeholder=\"Enter Email\"\n              v-model.trim=\"email\"\n              :class=\"{ error: submitted && $v.email.$invalid }\"\n            />\n          </div>\n          <div v-if=\"submitted\">\n            <div v-if=\"!$v.email.required\">\n              <p class=\"text-red text-left\">Email is required</p>\n            </div>\n            <div v-if=\"!$v.email.email\">\n              <p class=\"text-red text-left\">Invalid Email</p>\n            </div>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"profile\" class=\"float-left\">Profile Image:</label>\n            <input\n              type=\"file\"\n              class=\"form-control\"\n              id=\"profile\"\n              name=\"profile\"\n              @change=\"profileImage\"\n              :class=\"{ error: submitted && this.profile == '' }\"\n            />\n          </div>\n          <div v-if=\"submitted\">\n            <div v-if=\"this.profile == ''\">\n              <p class=\"text-red text-left\">Profile is required</p>\n            </div>\n          </div>\n          <button type=\"submit\" class=\"btn btn-primary mr10\">Registration</button>\n          <a @click=\"$router.push('login')\">Login</a>\n        </fieldset>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { required, email } from 'vuelidate/lib/validators';\nimport AuthService from '../../services/auth.service';\n\nexport default {\n  data() {\n    return {\n      name: '',\n      email: '',\n      profile: '',\n      submitted: false,\n      authService: new AuthService(),\n      error: ''\n    };\n  },\n  validations: {\n    name: {\n      required\n    },\n    email: {\n      required,\n      email\n    },\n    profile: {\n      required\n    }\n  },\n  name: 'Register',\n  methods: {\n    async register() {\n      try {\n        this.submitted = true;\n        if (this.$v.$invalid) {\n          return;\n        }\n        console.log(this.email, this.name, this.profile);\n\n        const formdata = new FormData();\n        formdata.append('user_name', this.name);\n        formdata.append('user_email', this.email);\n        formdata.append('profile_image', this.profile);\n        const url = 'http://180.149.241.208:3000/registration';\n        const result = await this.authService\n          .postRequest(url, formdata)\n          .catch((error) => {\n            console.log(error.response);\n            this.error = error.response;\n            if (this.error.data.success === 'false') {\n              alert(this.error.data.message);\n            }\n          });\n        if (result.data.success === 'true') {\n          alert(result.data.message);\n          this.$router.push('/login');\n        }\n        console.log('result', result);\n      } catch (error) {\n        console.log('error', error);\n      }\n    },\n    profileImage(event) {\n      const image = event.target.files[0];\n      const reader = new FileReader();\n      reader.readAsDataURL(image);\n      reader.onload = (e) => {\n        this.profile = e.target.result;\n      };\n    },\n  },\n};\n</script>\n\n<style  scoped>\n.error {\n  border: 1px solid red;\n}\n.text-red {\n  color: red;\n}\n</style>\n"],"sourceRoot":"src/views/auth"}]}